        "release": "npm publish ./dist/ngx-float-ui",
        "release:beta": "npm publish ./dist/ngx-float-ui --tag beta",
        "bump:patch": "gulp bump:patch",
        "bump:minor": "gulp bump:minor",
        "bump:major": "gulp bump:major"
