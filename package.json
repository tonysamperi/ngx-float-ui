        "ng": "ng",
        "start": "ng serve",
        "test": "ng test ngx-float-ui",
        "test:ci": "ng test ngx-float-ui --browsers=ChromeHeadlessCustom --watch=false",
        "test:demo": "ng test ngx-float-ui --browsers=Chrome",
